from unittest.mock import MagicMock, patch


def create_mocks():
    return [MagicMock(), MagicMock(), MagicMock()]


class Testing:

    def __init__(self):
        self.mock_list = self.get_mock_list()

    def get_mock_list(self):
        return [i for i in range(5)]


mock_list = create_mocks()

with patch.object(Testing, 'get_mock_list', return_value=create_mocks()):
    test = Testing()

    for i in test.mock_list:
        i.connect()

    for i in test.mock_list:
        print(i.connect.assert_called_once())
    
    # print(i.connect.assert_called_once())