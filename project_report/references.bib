}

@article{zhaoInexpensiveSystemDeterministic2020,
  title = {An Inexpensive System for the Deterministic Transfer of {{2D}} Materials},
  author = {Zhao, Qinghua and Wang, Tao and Ryu, Yu Kyoung and Frisenda, Riccardo and Castellanos-Gomez, Andres},
  date = {2020-01-23},
  journaltitle = {J. Phys. Mater.},
  volume = {3},
  number = {1},
  pages = {016001},
  issn = {2515-7639},
  doi = {10.1088/2515-7639/ab6a72},
  url = {https://iopscience.iop.org/article/10.1088/2515-7639/ab6a72},
  urldate = {2023-01-07},
  abstract = {Abstract             The development of systems for the deterministic transfer of two-dimensional (2D) materials have undoubtedly contributed to great advancements in 2D materials research. In fact, they have made it possible to fabricate van der Waals heterostructures and 2D materials-based devices with complex architectures. Nonetheless, as far as we know, the number of existing papers providing enough details to reproduce these systems by other research groups is very scarce in the literature. Moreover, these systems typically require the use of expensive optical and mechanical components hampering their applicability in research groups with a low budget. Here, we demonstrate how a deterministic placement system for 2D materials, set up with full capabilities, can be implemented for under 900 € -- which can be easily implemented in labs and educational labs with low budgets.},
  langid = {english},
  file = {C\:\\Users\\baskl\\Zotero\\storage\\DQJNXNYZ\\Zhao et al. - 2020 - An inexpensive system for the deterministic transf.pdf}
}
