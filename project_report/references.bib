  author = {Taghavi, Najme S. and Gant, Patricia and Huang, Peng and Niehues, Iris and Schmidt, Robert and Michaelis de Vasconcellos, Steffen and Bratschitsch, Rudolf and García-Hernández, Mar and Frisenda, Riccardo and Castellanos-Gomez, Andres},
